<p id="notice"><%= notice %></p>

<h1>Mitch</h1>

<table>
  <thead>
    <tr>
      <th>song</th>
      <th>Genre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= link_to "#{song.title} by #{song.artist}", song %></td>
        <td><%= song.genre %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
      </tr>
    <% end %>
  </tbody>
</table>




